issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
number = {Supplement 1},
pages = {8597--8604},
pmid = {17494740},
title = {{The frailty of adaptive hypotheses for the origins of organismal complexity}},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.0702207104},
volume = {104},
year = {2007}
