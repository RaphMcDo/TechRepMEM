title = {{Role of Epistasis and Overdominance in Stability of Equilibria With Selection.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=222674{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {45},
