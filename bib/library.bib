issn = {02784343},
journal = {Continental Shelf Research},
keywords = {Bay of Fundy,Seascape map,Surficial geology,Tidal scour},
pages = {53--63},
publisher = {Elsevier},
title = {{Geologic insights from multibeam bathymetry and seascape maps of the Bay of Fundy, Canada}},
url = {http://dx.doi.org/10.1016/j.csr.2013.12.015},
volume = {83},
