journal = {Nature},
keywords = {Animals,Aquatic Organisms,Aquatic Organisms: physiology,Biodiversity,Biomass,Conservation of Natural Resources,Conservation of Natural Resources: economics,Conservation of Natural Resources: legislation {\&} j,Conservation of Natural Resources: methods,Conservation of Natural Resources: statistics {\&} nu,Coral Reefs,Ecology,Ecology: economics,Ecology: legislation {\&} jurisprudence,Ecology: methods,Ecology: statistics {\&} numerical data,Ecosystem,Fisheries,Fisheries: legislation {\&} jurisprudence,Fisheries: standards,Fisheries: statistics {\&} numerical data,Fishes,Fishes: physiology,Marine Biology,Marine Biology: economics,Marine Biology: legislation {\&} jurisprudence,Marine Biology: methods,Marine Biology: statistics {\&} numerical data,Seawater,Sharks,Silicon Dioxide,Time Factors},
