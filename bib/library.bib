@article{Pineda2007a,
abstract = {Author Posting. {\textcopyright} Oceanography Society, 2007. This article is posted here by permission of Oceanography Society for personal use, not for redistribution. The definitive version was published in Oceanography 20, 3 (2007): 22-39.},
annote = {Difference between larval transport and dispersal:

Transport: horizontal translocation between two points (ignores vertical dimension for simplicity which is very important since different vertical positions might encounter different). Seems simple but wide variability at multiple scales makes it very hard to measure.

Dispersal: spread of larvae from spawning source to settlement source. Broad dispersal requires lots of transport (restricted dispersal does not imply little transport). 

Larval mortality understudied (highest mortality for marine populations occur at early life stages).

Difference between population connectivity (exchange of individuals among georgraph separated subpop) and reproductive population connectivity (dispersal of individuals among subpop that survive to reproduce, includes larval dispersal but also influenced by post-settlement mortality).

Shorelines make things more complex. Tidal ellipses get compressed near coast in different ways depending where they are, there are flows (Gulf Stream) flowing parallel, freshwater runoff, etc.

Large-scale physical process can also influence smaller scale process and directly move the larvae. Shallow thermocline creates shear (may restrict larval transport) so modifications of thermocline (such as El Nino which creates weak stratification) can lead to dispersal variability. e.g. El Nino results in decreased water-column stratification which may result in decreased onshore larval transport.

Small-scale (temporal and space) events and processes are very hard to identify their effects on larval transport.

Although underestimated before, there have been studies showing that larvae present behaviours which can directly affect their dispersal.

Problems with sampling is that, since planned ahead, transport might not be occuring. Adaptive sampling (in response to an event or some specific circumstances) is a good solution to that but has to be hypothesis-based and is challenging, expensive and demanding.

Multiple examples proving that most larvae are not passively dispersed (compared to coal residue, dye samples, etc.).

Initial thought of broad dispersal is probably not accurate due to unexpected high levels of self-recruitment, high larval mortality rates and restricted scales of larval trasnport.

Early life history characteristics appear to influence larval survival which influences dispersal. Also seem that larger at a given age, faster growth and move through early stages more rapidly tend to be the larvae that survive to later stages.

Characteristics from larvae stage can carry over into juvenile stage but not always a positive thing (fish larvae with short pelagic duration escape predation but become smaller settlers)},
author = {Pineda, Jes{\'{u}}s and Hare, Jonathan A. and Sponaugle, Su},
doi = {10.5670/oceanog.2007.27},
file = {::},
isbn = {1042-8275},
issn = {10428275},
journal = {Oceanography},
number = {3},
pages = {22--39},
pmid = {8113270},
title = {{Larval Transport and Dispersal in the Coastal Ocean and Consequences for Population Connectivity}},
url = {http://tos.org/oceanography/article/larval-transport-and-dispersal-in-the-coastal-ocean-and-consequences-for-po},
volume = {20},
year = {2007}
}
@article{Magris2014a,
abstract = {Most applications of systematic conservation planning have not effectively incorporated biological processes or dynamic threats. We investigated the extent to which connectivity and climate change have been considered in an ecologically meaningful way in marine conservation planning, as an attempt to help formulate conservation objectives for population persistence, over and above representation. Our review of the literature identified 115 marine planning studies that addressed connectivity and 47 that addressed the effects of climate change. Of the statements identified that related to goals and objectives, few were quantitative and justified by ecological evidence for either connectivity (13{\%}) or climate change (8.9{\%}). Most studies addressing connectivity focused on spatial design (e.g. size and spacing) of marine protected areas (MPAs) or clustering of planning units. Climate change recommendations were primarily based on features related to MPA placement (e.g. preferences for areas relatively resilient and resistant to climate change impacts). Quantitative methods to identify spatial or temporal dynamics of features related to connectivity and/or climate change (e.g. functionally well-connected or thermal refugia areas) were rare, and these accounted for the majority of ecologically justified statements. Given these shortcomings in the literature, we outline a framework for setting marine conservation planning objectives that describes six key approaches to more effectively integrate connectivity and climate change into conservation plans, aligning opportunities and minimizing trade-offs between both issues. ?? 2013 Elsevier Ltd.},
annote = {Very few MPA approaches have been based on expected changes resulting from climate change and its effect on species and ecosystems. In regards to connectivity, climate change will modify larval disperal pathways but will also change spawning timing, larval transport, mortality and behaviour. However connectivity can enhance recovery from those changes. 

Seems that a lot of the studies done on climate change, connectivity and MPAs are more qualitative in nature and not often directly related to measurable ecological factors. 

Underlines that qualitative conclusions should be used to inform more specific conservation objectives although preference should be accorded by those that are backed by an understanding of ecological processes which support these qualitative statements.

Many types of surrogates for diversity have been proposed but not a lot have evidence backing them. Physical seascape attribute is one and could be surrogate for ecological processes important for population persistence but few empirical studies have looked into this.},
author = {Magris, Rafael A. and Pressey, Robert L. and Weeks, Rebecca and Ban, Natalie C.},
doi = {10.1016/j.biocon.2013.12.032},
file = {::},
isbn = {0006-3207},
issn = {00063207},
journal = {Biological Conservation},
keywords = {Biodiversity conservation,Dispersal,Global warming,Marine protected area,Marine reserve,Marine reserve design},
pages = {207--221},
publisher = {Elsevier Ltd},
title = {{Integrating connectivity and climate change into marine conservation planning}},
url = {http://dx.doi.org/10.1016/j.biocon.2013.12.032},
volume = {170},
year = {2014}
}
