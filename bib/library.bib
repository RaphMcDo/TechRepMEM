pages = {27--45},
title = {{Identifiable state-space models: A case study of the Bay of Fundy sea scallop fishery}},
volume = {47},
year = {2019}
}
@article{White2015a,
abstract = {Models for marine reserve design have been developed primarily with 'reef fish' life histories in mind: sedentary adults in patches connected by larval dispersal. However, many fished species undertake ontogenetic migrations, such as from nursery grounds to adult spawning habitats, and current theory does not fully address the range of reserve options posed by that situation. I modelled a generic species with ontogenetic migration to investigate the possible benefits of reserves under three alternative scenarios. First, the fishery targets adult habitat, and reserves can sustain yields under high exploitation, unless habitat patches are well connected. Second, the fishery targets the nursery, and reserves are highly effective, regardless of connectivity patterns. Third, the fishery targets both habitats, and reserves only succeed if paired on adjacent, well-connected nursery and adult patches. In all cases, reserves can buffer populations against overexploitation but would not enhance fishery yield beyond that achievable by management without reserves. These results summarize the general situations in which management using reserves could be useful for ontogenetically migrating species, and the type of connectivity data needed to inform reserve design.},
author = {White, J. W.},
doi = {10.1098/rsbl.2014.0511},
file = {:C$\backslash$:/Users/mcdonaldra/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/White - 2015 - Marine reserve design theory for species with ontogenetic migration.pdf:pdf},
issn = {1744-9561},
journal = {Biology Letters},
number = {1},
pages = {20140511--20140511},
pmid = {25631225},
title = {{Marine reserve design theory for species with ontogenetic migration}},
url = {http://rsbl.royalsocietypublishing.org/cgi/doi/10.1098/rsbl.2014.0511},
volume = {11},
year = {2015}
}
@article{Bowie1993,
abstract = {The family Pectinaceae are the only bivalves that have been well documented as swimmers, although Ensis minor has been observed to swim. The behaviour was once regarded only as an escape response, but some studies suggest seasonal migrations are involved. Demont (1990) suggests that they swim at the natural frequency of the shell-hinge system for more efficient use of energy. The family Pectinaceae have acquired the ability to swim through the development of a specialized hinge ligament. The hinge ligament is made up of two parts: the inner and outer ligaments. The inner ligament is located ventrally and acts as a compression spring that forces the shells apart when the adductor muscle relaxes. The outer ligament is located dorsally and connects the two valves, acting as a tension spring when the valves are closed. In other bivalves, such as Mytilus, Ostrea or Tellina, the inner hinge ligament is calcified throughout, but in Pecten magellanicus it is only calcified at the fusion layer, where it attaches to the shell. The material that comprises the inner hinge ligament, abductin, is one of the few known biomaterials that exists as a one-phase amorphous polymer system. Abductin is different from other rubber-like proteins in that it contains large amounts of the amino acid methionine, while the other materials contain little or none . For such a hinge to function efficiently as a spring, it must be compliant enough to be deformed by the muscle and have a low energy loss, or damping, at functional frequencies. Alexander (1966) showed that the elasticity of the inner hinge ligament is mainly due to entropy changes, but no studies have quantified the mechanical properties of the intact hinge over normal physiological conditions.},
author = {Bowie, Michael A and Layes, Jonathan D and Demont, M Edwin},
file = {:C$\backslash$:/Users/mcdonaldra/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Bowie, Layes, Demont - 1993 - Damping in the Hinge of the Scallop Placopecten Magellanicus.pdf:pdf},
issn = {0022-0949},
journal = {Journal of Experimental Biology},
keywords = {Abductine,Amortiguaci{\'{o}}n,Amortissement,Bivalvia,Damping,Invertebrata,Locomoci{\'{o}}n,Locomotion,Mechanical properties,Mollusca,Nado,Nage,Placopecten magellanicus,Propiedad mec{\'{a}}nica,Propri{\'{e}}t{\'{e}} m{\'{e}}canique,Swimming},
number = {1},
pages = {311--315},
title = {{Damping in the Hinge of the Scallop Placopecten Magellanicus}},
volume = {175},
year = {1993}
}
