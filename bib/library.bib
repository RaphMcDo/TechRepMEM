}
@article{Bailey2016a,
abstract = {Canada's ocean ecosystem health and functioning is critical to sustaining a strong maritime economy and resilient coastal communities. Yet despite the importance of Canada's oceans and coasts, federal ocean policy and management have diverged substantially from marine science in the past decade. In this paper, key areas where this is apparent are reviewed: failure to fully implement the Oceans Act, alterations to habitat protections historically afforded under Canada's Fisheries Act, and lack of federal leadership on marine species at risk. Additionally, the capacity of the federal government to conduct and communicate ocean science has been eroded of late, and this situation poses a significant threat to current and future oceans public policy. On the eve of a federal election, these disconcerting threats are described and a set of recommendations to address them is developed. These trends are analyzed and summarized so that Canadians understand ongoing changes to the health of Canada's oceans and the role that their elected officials can play in addressing or ignoring them. Additionally, we urge the incoming Canadian government, regardless of political persuasion, to consider the changes we have documented and commit to aligning federal ocean policy with ocean science to ensure the health of Canada's oceans and ocean dependent communities.},
author = {Bailey, Megan and Favaro, Brett and Otto, Sarah P. and Charles, Anthony and Devillers, Rodolphe and Metaxas, Anna and Tyedmers, Peter and Ban, Natalie C. and Mason, Taylor and Hoover, Carie and Duck, Thomas J. and Fanning, Lucia and Milley, Chris and Cisneros-Montemayor, Andr??s M. and Pauly, Daniel and Cheung, William W L and Cullis-Suzuki, Sarika and Teh, Louise and Sumaila, U. Rashid},
doi = {10.1016/j.marpol.2015.10.002},
file = {::},
isbn = {0790-0627},
issn = {0308597X},
journal = {Marine Policy},
keywords = {Canada,Evidence-based policies,Federal policy,Fisheries Act,Marine management,Ocean science,Science communication,Species at Risk Act},
pages = {53--60},
publisher = {Elsevier},
title = {{Canada at a crossroad: The imperative for realigning ocean policy with ocean science}},
url = {http://dx.doi.org/10.1016/j.marpol.2015.10.002},
volume = {63},
year = {2016}
}
