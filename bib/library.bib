issn = {01657836},
journal = {Fisheries Research},
keywords = {Delta distribution,Gamma distribution,Generalized linear mixed model,Trawl survey},
pages = {251--264},
title = {{A generalized linear mixed model analysis of a multi-vessel fishery resource survey}},
volume = {70},
