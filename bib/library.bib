file = {:C$\backslash$:/Users/mcdonaldra/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/White et al. - 2017 - Assessing the effectiveness of a large marine protected area for reef shark conservation.pdf:pdf},
isbn = {00063207},
issn = {00063207},
journal = {Biological Conservation},
keywords = {Automatic identification system,Carcharhinus amblyrhynchos,Fisheries,Marine protected areas,Satellite telemetry,Vessel tracking},
pages = {64--71},
publisher = {Elsevier Ltd},
