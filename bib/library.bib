@article{JoelLSachsUlrichGMuellerThomasPWilcox2004,
abstract = {Darwin recognized that natural selection could not favor a trait in one species solely for the benefit of another species. The modern, selfish-gene view of the world suggests that cooperation between indi- viduals, whether of the same species or different species, should be especially vulnerable to the evolution of noncooperators. Yet, cooperation is prevalent in nature both within and between species. What special circumstances or mechanisms thus favor cooperation? Currently, evolutionary biology offers a set of disparate explanations, and a general framework for this breadth of models has not emerged. Here, we offer a tripartite structure that links previously disconnected views of cooperation.We distinguish three general models by which cooperation can evolve and be maintained: (i) directed reciprocation— cooperation with individuals who give in return; (ii) shared genes—cooperation with relatives (e.g., kin selection); and (iii) byproduct benefits—cooperation as an incidental consequence of selfish action. Each general model is further subdivided. Several renowned examples of cooperation that have lacked explanation until recently—plant-rhizobium symbioses and bacteria-squid light organs—fit squarely within this framework. Natural systems of cooperation often involve more than one model, and a fruitful direction for future research is to understand how these models interact to maintain cooperation in the long term. A},
author = {{Joel L Sachs, Ulrich G Mueller, Thomas P Wilcox}, and James J Bull},
doi = {10.1086/431023},
file = {:C$\backslash$:/Users/mcdonaldra/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Joel L Sachs, Ulrich G Mueller, Thomas P Wilcox - 2004 - The Quarterly Review of Biology The evolution of cooperation.pdf:pdf},
issn = {0036-8075},
journal = {The Quarterly Review of Biology},
keywords = {byproducts,directed reciprocation,fidelity feedback,mutualism,partner,partner choice,shared genes,symbiosis},
number = {2},
pages = {135--160},
pmid = {785523},
title = {{The Quarterly Review of Biology The evolution of cooperation.}},
url = {http://www.jstor.org/stable/10.1086/383541 .},
volume = {79},
year = {2004}
}
