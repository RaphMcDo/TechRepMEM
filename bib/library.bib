@article{Scheibling1986a,
abstract = {Recurrent outbreaks of disease between 1980 and 1983 caused catastrophic mortality of sea urchins ({\textgreater}260,000 t fresh weight) along 280 km (straight line distance) of the Atlantic coast of Nova Scotia. The complete elimination of sea urchins and concomitant development of fleshy macroalgal communities have occurred along different parts of this coast in different years. Macroalgal communities in areas where sea urchins died off 1, 3 and 4 years previously are compared to existing sea urchin-dominated barren grounds and to a mature kelp bed without sea urchins. Changes in macroalgal cover and species composition, and increases in biomass, density and size of kelp (Laminaria) species, characterize the succession from barren grounds to 3- and 4-year-old kelp beds. The greatest change occurred between one and three years following sea urchin mass mortality. Within 3 years, kelp beds attained a level of biomass (7.6 kg m-2) comparable to that of mature beds. Recovery of sea urchin populations via recruitment of planktonic larvae has been slow and spatially variable. Large-scale reciprocal fluctuations in kelp and sea urchin biomass may characterize the trajectory of a dynamic system which cycles between two alternate community states: kelp beds and sea urchin-dominated barren grounds. Periodic decimation of sea urchin populations by disease may be an important mechanism underlying this cyclicity.},
author = {Scheibling, Robert},
file = {::},
journal = {Atlantic},
number = {November 1983},
pages = {186--198},
title = {{Oecologia of sea urchins ( Strongylocentrotus droebachiensis ) along the Atlantic coast of Nova Scotia}},
year = {1986}
}
@article{Sciberras2013a,
abstract = {Marine Protected Areas (MPAs) encompass a range of protection levels, from fully protected no-take areas to restriction of only particular activities, gear types, user groups, target species or extraction periods. We synthesized the results of empirical studies that compared partially protected areas (PPA) to (i) no-take marine reserves (NTR) and (ii) to open access areas (Open), to assess the potential benefits of different levels of protection for fish and invertebrate populations.},
annote = {MPAs focus on ecosystem-based conservation instead of classic single species management.

Not a lot has been shown for PPAs, some research pointing towards improvements others showing no difference in comparison to unprotected zones.

Uses density, biomass, species richness and body size to assess if MPAs are successful in protecting fish and invert populations. Also tries to see if there's a difference in the effect on target and non-target species.

Fish density was higher inside PPA than in Open area for most of studies they looked at. However there were a lot of variability between all of those studies.

PPA had different effects depending on taxonomy. Lobster and scallops had positive effects in comparison to outside (scallop was more consistent and better). Echninoid seemed to have lower densities in PPA. 

Since NTR are not always politically or socio-economically good, PPA appear to have some positive protection effects on target species although NTR are better at it. 

Part of the reason why very large PPA might not be efficient is because they're very hard to properly patrol and make sure no infringement is done. It might also be because the sampling done in most of the large ones only represented a tiny portion of the MPA and thus might not be representative.},
author = {Sciberras, Marija and Jenkins, Stuart R and Kaiser, Michel J. and Hawkins, Stephen J and Pullin, Andrew S},
doi = {10.1186/2047-2382-2-4},
file = {::},
issn = {2047-2382},
journal = {Environmental Evidence},
keywords = {exploitation status,fish,invertebrates,marine protected areas,mpa design,protection level,weighted},
mendeley-tags = {exploitation status,fish,invertebrates,marine protected areas,mpa design,protection level,weighted},
pages = {4},
title = {{Evaluating the biological effectiveness of fully and partially protected marine areas}},
url = {http://www.environmentalevidencejournal.org/content/2/1/4},
volume = {2},
year = {2013}
}
