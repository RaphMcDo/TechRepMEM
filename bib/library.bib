issn = {1543-5938},
journal = {Annual Review of Environment and Resources},
number = {1},
pages = {31--68},
pmid = {1121},
title = {{Marine reserves and ocean neighborhoods:The Spatial Scale of Marine Populations and Their Management}},
url = {http://arjournals.annualreviews.org/doi/abs/10.1146/annurev.energy.29.062403.102254{\%}5Cnhttp://www.annualreviews.org/doi/pdf/10.1146/annurev.energy.29.062403.102254},
volume = {29},
