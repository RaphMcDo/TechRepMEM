keywords = {abbre6iations,biogeography,canonical correspondence analysis,cart,cc,cca,classi cation and regression,climate change,dem,digital elevation model,ecological niche factor analysis,enfa,gam,generalized additive model,generalized linear model,glm,least squares,ls,model,model calibration,model formulation,pca,plant ecology,principal component analysis,rda,redundancy analysis,species models,trees,vegetation models},
number = {2-3},
pages = {147--186},
title = {{Predictive habitat distribution models in ecology}},
