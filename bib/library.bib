issn = {0706-652X},
journal = {Canadian Journal of Fisheries and Aquatic Sciences},
number = {April 2016},
pages = {1--14},
title = {{Accounting for spatiotemporal variation and fisher targeting when estimating abundance from multispecies fishery data {\textless}sup{\textgreater}1{\textless}/sup{\textgreater}}},
url = {http://www.nrcresearchpress.com/doi/10.1139/cjfas-2015-0598},
volume = {1807},
