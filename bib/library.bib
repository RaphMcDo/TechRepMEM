@article{Cook2010,
abstract = {A series of short, laboratory-based trials was conducted on striped bass Morone saxatilis from the Shubenacadie River, Nova Scotia, Canada, starting with wild-caught eggs and proceeding through to the early juvenile stage. The levels of the experimental factors-salinity (0-35 parts per thousand) and temperature (10-30 degrees C)-spanned the range occurring in the natal estuary. Survival of eggs to 1 d posthatch (dph) was highest, more than 60{\%}, between 2 parts per thousand and 20 parts per thousand salinity and was reduced significantly (to {\textless}50{\%}) only in salinities of 30 parts per thousand or greater. Similarly, survival of prefeeding larvae was reduced only at salinities above 30 parts per thousand, suggesting that their salinity tolerance is higher than that of U.S. populations. Moreover, 1-7-dph larvae were tolerant to temperature decreases that are lethal to other populations, with around 40{\%} surviving between 10 degrees C and 14 degrees C at intermediate salinities. Older, 23-dph larvae were less tolerant to cold, with fewer than 20{\%} surviving 7 d at 10 degrees C, but these larvae thrived at high temperature, exhibiting highest survival and growth at 26 degrees C across salinities from 1 parts per thousand to 35 parts per thousand. Growth of early juvenile stages (55-104 dph) was independent of salinity (1-30 parts per thousand) and was highest between 26 degrees C and 30 degrees C, suggesting a temperature optimum similar to that of U. S. populations},
author = {Cook, A. M. and Duston, J. and Bradford, R. G.},
doi = {10.1577/T08-173.1},
file = {:C$\backslash$:/Users/mcdonaldra/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Cook, Duston, Bradford - 2010 - Temperature and Salinity Effects on Survival and Growth of Early Life Stage Shubenacadie River Striped B.pdf:pdf},
isbn = {0002-8487},
issn = {0002-8487},
journal = {Transactions of the American Fisheries Society},
number = {3},
pages = {749--757},
title = {{Temperature and Salinity Effects on Survival and Growth of Early Life Stage Shubenacadie River Striped Bass}},
url = {http://www.tandfonline.com/doi/abs/10.1577/T08-173.1},
volume = {139},
