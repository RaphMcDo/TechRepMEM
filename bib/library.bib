file = {:C$\backslash$:/Users/mcdonaldra/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/B{\oe}uf, Payan - 2001 - How should salinity influence fish growth.pdf:pdf},
isbn = {1532-0456},
issn = {15320456},
journal = {Comparative Biochemistry and Physiology - C Toxicology and Pharmacology},
keywords = {Energetic cost,Fish growth,Metabolism,Osmoregulation,Oxygen consumption,Salinity, Growth hormonal control},
number = {4},
pages = {411--423},
pmid = {11738629},
title = {{How should salinity influence fish growth?}},
volume = {130},
year = {2001}
}
