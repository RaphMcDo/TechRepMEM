@article{Navarro2021,
abstract = {Marine recreational fisheries (MRFs) are often highly spatially heterogenous, with effort concentrated into small areas, and fisheries spanning large environmental gradients. However, spatially resolved catch data is rarely collected in MRFs, preventing the study of spatial heterogeneity in catch. This study uses recreational catch reported in 10 * 10 nm blocks across eight degrees of latitude in Western Australia to map spatial predictions of the probability of a recreational catch on an average trip for two key species: West Australian dhufish (Glaucosoma hebraicum) and snapper (Chrysophrys auratus). Two spatial modelling techniques are compared for the analysis, generalized additive mixed models (GAMMs) and boosted regression trees (BRTs). We find that BRTs outperform GAMMs, but performance gains are small. We also find marked spatial variations in recreational catch probabilities: high catches of dhufish are found in the north of the study area, and low catches in the Perth Metropolitan area and in the south; snapper catches are highest in the north and low in the south. These patterns are used to identify important spatial processes in the fishery. The analysis also suggests that modelling approach (GAMMs or BRTs) has only a minor effect on outcomes of spatial catch analysis in MRFs.},
author = {Navarro, Matthew and Hailu, Atakelty and Langlois, Tim and Ryan, Karina L. and Kragt, Marit E.},
doi = {10.1093/ICESJMS/FSZ123},
file = {:C$\backslash$:/Users/mcdonaldra/Downloads/fsz123.pdf:pdf},
issn = {10959289},
journal = {ICES Journal of Marine Science},
keywords = {Angling,Chrysophrys auratus,Glaucosoma hebraicum,Recreational fishing,Western Australia},
number = {6},
pages = {2216--2225},
title = {{Determining spatial patterns in recreational catch data: A comparison of generalized additive mixed models and boosted regression trees}},
volume = {77},
year = {2020}
}
@article{Underwood2009,
abstract = {Coral reefs are in decline worldwide, and marine reserve networks have been advocated as a powerful management tool for maximizing the resilience of coral communities to an increasing variety, number, and severity of disturbances. However, the effective design of reserves must account for the spatial scales of larval dispersal that affect the demography of communities over ecological time frames. Ecologically relevant distances of dispersal were inferred from DNA microsatellite data in a broadcast-spawning (Acropora tenuis) and a brooding (Seriatopora hystrix) coral at isolated reef systems off northwest Australia. Congruent with expectations based on life histories, levels of genetic subdivision among populations were markedly higher in the brooder than in the broadcast spawner. Additionally, significant subdivision for both species between systems ({\textgreater}100 km), and between ({\textgreater}10 km) or within reefs ({\textless}10 km) within systems, indicated that many reefs or reef patches are demographically independent. There was also a clear distinction in the scale of genetic structure between the different systems; at the more geographically complex of the systems, a much finer scale structure was detected in both species. This suggested that the hydrodynamics associated with these complex reefs restrict distances regularly traveled by larvae. The primary implication is that short-term recovery of these coral communities after severe disturbance requires the input of larvae from viable communities kilometers to a few tens of kilometers away. Therefore, to be self-sustaining, we suggest that coral reef protected areas need to be large enough to encompass these routine dispersal distances. Further, to facilitate recovery from severe disturbances, protected areas need to be replicated over these spatial scales. However, specific designs also need to account for size, complexity, and isolation of reefs, which will either restrict or enhance dispersal within this range.},
author = {Underwood, Jim N. and Smith, Luke D. and {Van Oppen}, Madeleine J.H. and Gilmour, James P.},
doi = {10.1890/07-1461.1},
file = {:C$\backslash$:/Users/mcdonaldra/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Underwood et al. - 2009 - Ecologically relevant dispersal of corals on isolated reefs Implications for managing resilience.pdf:pdf},
isbn = {1051-0761},
issn = {10510761},
journal = {Ecological Applications},
keywords = {Acropora tenuis,Disturbance,Gene flow,Genetic connectivity,Marine protected areas,Marine reserves,Microsatellites,Northwest Australia,Recruitment,Reef-building coral,Rowley shoals,Scott reef,Scriatopora hystrix},
number = {1},
pages = {18--29},
pmid = {19323171},
title = {{Ecologically relevant dispersal of corals on isolated reefs: Implications for managing resilience}},
volume = {19},
year = {2009}
}
