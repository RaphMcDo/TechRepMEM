journal = {Journal of Experimental Marine Biology and Ecology},
keywords = {Identification of species,Larvae,Mitochondrial DNA,PCR,Predation,Sand shrimp,Stomach contents,Stone flounder},
number = {2},
pages = {153--163},
pmid = {1594},
title = {{Identification of consumed stone flounder, Kareius bicoloratus (Basilewsky), from the stomach contents of sand shrimp, Crangon affinis (De Haan) using mitochondrial DNA analysis}},
