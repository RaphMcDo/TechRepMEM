issn = {0275-5947},
journal = {North American Journal of Fisheries Management},
number = {2},
pages = {335--351},
title = {{ A Spatially Explicit Open-Ocean DeLury Analysis to Estimate Gear Efficiency in the Dredge Fishery for Sea Scallop Placopecten magellanicus }},
volume = {24},
year = {2004}
}
