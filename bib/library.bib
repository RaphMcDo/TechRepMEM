doi = {10.1139/f95-241},
file = {:C$\backslash$:/Users/mcdonaldra/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Letcher et al. - 1996 - Variability in survival of larval fish disentangling components with a generalized individual-based model.pdf:pdf},
