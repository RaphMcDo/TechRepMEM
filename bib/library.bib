journal = {Journal of Shellfish Research},
keywords = {Carcinus maenas,Hemigrapsus sanguineus,Homarus americanus,aggression,crab,invasive species,lobster,predator-prey interactions},
number = {3},
pages = {1091--1096},
title = {{Differential Response of the American Lobster Homarus americanus to the Invasive Asian Shore Crab Hemigrapsus sanguineus and Green Crab Carcinus maenas}},
volume = {34},
year = {2015}
}
