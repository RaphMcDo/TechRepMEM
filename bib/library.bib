@article{Margules2007a,
abstract = {Systematic Conservation Planning provides a clear, comprehensive guide to the process of deriving conservation area networks that will represent biodiversity in cost-effective ways. The measurement of biodiversity, the design of field collections to sample biodiversity and data treatment methods together provide baseline information on the distribution patterns of biodiversity. The book then describes methods for identifying biodiversity priority areas underpinned by the concept of complementarity. It goes on to describe how biodiversity targets can be set and how multi-criteria analyses can incorporate costs and other constraints into the area selection process, enabling trade-offs between production and protection to be made. The book finishes with a series of case studies and a conclusion that emphasizes the socio-economic and cultural contexts in which conservation planning takes place. A clear procedure is provided for identifying biodiversity priority areas, underpinned by current best science practice and sound common sense, making this volume of interest not only to graduate students and academic conservation biologists, but also planners and decision-makers dealing with natural resource management together with conservation NGOs.},
annote = {Creating reserves has usually not been done in a systematic ways organised around specific goals, although that is much more the case now. Land that has economic value usually does not get any protection (leading to no protection to the biodiversity housed inside those areas). Protected areas are usually places that had no economic value or were very remote at the time of the protection.

A systematic way of determining the importance of area is build. Stage 1 is to measure and map biodiversity. It is possible to relate environmental variables as a good estimator of species spatial distribution (some evidence of this) so would not have to survey every area ever. Still need some biological information about area in question and data from herbarium or collection not very reliable indicators (biased). 

Stage 2: identify conservation goals for the planning region. Most have been directed towards areas on basis of occurence of species (with some using probabilities of occurence).

Stage 3: Review existing reserves. Basically figure out what features are already represented in this and what is not or only partially. Also take into account the likelihood of certain species or habitats of going extinct without conservation action. Its gap analyses incorporating threat can reveal if there are biases in the current reserves that could inhibit effective implementation of reserves.

Stage 4: select additional reserves. 5 types of constraints to choose: Irreplaceability of habitat/species, cost (commercial opportunity cost, acquisition cost, ongoing management cost, etc.), commitments (areas that have to be selected anyways such as pre-existing reserves and areas with rare and threatened species), masks (areas to be excluded) and preferences (certain characteristics being preferred over others like low human density, etc). 

Stage 5: implementation on the ground. Important to identify most appropriate or feasible form of management for targeted areas. If area unexpected degraded or difficult to protect, return to stage 4 to identify replacements if they exist.Third, need to identify appropriate timing. 

Stage 6: management and monitoring of reserves.},
archivePrefix = {arXiv},
arxivId = {j.1744-7429.2008.00471.x},
author = {Margules, C. R. and Pressey, Robert L.},
doi = {10.1038/35012251},
eprint = {j.1744-7429.2008.00471.x},
file = {::},
isbn = {9780521878753$\backslash$r9780521703444},
issn = {00280836},
journal = {Ecology, biodiversity, and conservation},
keywords = {beschermingsgebieden,biodiversiteit,biodiversity,conservation,conservation areas,conservering,diversiteit,diversity,hulpbronnenbehoud,in situ conservation,in-situ conservering,planning,resource conservation},
number = {May},
pages = {270},
pmid = {10821285},
title = {{Systematic conservation planning}},
volume = {405},
year = {2007}
}
