file = {:C$\backslash$:/Users/mcdonaldra/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Harmon et al. - 2010 - Early bursts of body size and shape evolution are rare in comparative data.pdf:pdf},
isbn = {0014-3820},
issn = {00143820},
journal = {Evolution},
keywords = {Adaptive radiation,Brownian motion,Comparative methods,Model fitting,Phylogeny},
number = {8},
