issn = {01657836},
journal = {Fisheries Research},
keywords = {CPUE standardization,GAM,GLM-tree algorithm,Retrospective analysis,Spatial stratification,Virtual population analysis},
pages = {105315},
publisher = {Elsevier},
title = {{Spatiotemporal dynamics of the Pacific chub mackerel revealed by standardized abundance indices}},
