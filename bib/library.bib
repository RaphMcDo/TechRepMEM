journal = {Journal of Heredity},
number = {5},
pages = {372--387},
title = {{Classification of hypotheses on the Advantage of Amphimixis}},
url = {http://dx.doi.org/10.1111/j.1600-0706.2008.17164.x},
volume = {84},
year = {1993}
