number = {11},
pages = {1292--1300},
title = {{Isolation by oceanic distance and spatial genetic structure in an overharvested international fishery}},
volume = {23},
year = {2017}
}
@article{Teske2010,
abstract = {No-take marine protected areas (MPAs) are successful in protecting populations of many exploited fish species, but it is often unclear whether networks of MPAs are adequately spaced to ensure connectivity among reserves, and whether there is spillover into adjacent exploited areas. Such issues are particularly important in species with low dispersal potential, many of which exist as genetically distinct regional stocks. The roman, Chrysoblephus laticeps, is an overexploited, commercially important sparid endemic to South Africa. Post-recruits display resident behavior and occupy small home ranges, making C. laticeps a suitable model species to study genetic structure in marine teleosts with potentially low dispersal ability. We used multilocus data from two types of highly variable genetic markers (mitochondrial DNA control region and seven microsatellite markers) to clarify patterns of genetic connectivity and population structure in C. laticeps using samples from two MPAs and several moderately or severely exploited regions. Despite using analytical tools that are sensitive to detect even subtle genetic structure, we found that this species exists as a single, well-mixed stock throughout its core distribution. The high levels of connectivity identified among sites support the findings of previous studies that have indicated that inshore MPAs are an adequate tool for managing overexploited temperate reef fishes. Even though dispersal of adult C. laticeps out of MPAs is limited, the fact that the large adults in these reserves produce exponentially more offspring than their smaller counterparts in exploited areas makes MPAs a rich source of recruits. We nonetheless caution against concluding that the lack of structure identified in C. laticeps and several other southern African teleosts can be considered to be representative of marine teleosts in this region in general. Many such species are represented in more than one marine biogeographic province and may be comprised of regionally adapted stocks that require individual management.},
author = {Teske, P. R. and Forget, F. R.G. and Cowley, P. D. and von der Heyden, S. and Beheregaray, L. B.},
doi = {10.1007/s00227-010-1471-z},
file = {:C$\backslash$:/Users/mcdonaldra/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Teske et al. - 2010 - Connectivity between marine reserves and exploited areas in the philopatric reef fish Chrysoblephus laticeps (Tele.pdf:pdf},
isbn = {0025-3162},
issn = {00253162},
journal = {Marine Biology},
number = {9},
pages = {2029--2042},
title = {{Connectivity between marine reserves and exploited areas in the philopatric reef fish Chrysoblephus laticeps (Teleostei: Sparidae)}},
volume = {157},
year = {2010}
}
