journal = {Canadian Journal of Fisheries and Aquatic Sciences},
number = {9},
pages = {2063--2079},
title = {{Influence of origin on migration and survival of Atlantic salmon (Salmo salar) in the Bay of Fundy, Canada}},
url = {http://www.nrcresearchpress.com/doi/abs/10.1139/F08-119},
volume = {65},
